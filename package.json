  "scripts": {
    "eslint": "eslint ./ --fix",
    "prebuild": "rimraf dist/",
    "build": "tsc && tsc-alias",
    "start": "node -r dotenv/config dist/main.js",
    "start:dev": "tsx watch -r dotenv/config src/main.ts",
    "prisma:migration": "prisma db push",
    "prisma:seed": "tsx prisma/seeds/index.ts",
    "prisma:reset": "prisma db push --force-reset && tsx prisma/seeds/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "inversify": "^6.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.10",
    "nodemailer-express-handlebars": "^6.1.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.3",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.11.20",
    "@types/nodemailer": "^6.4.14",
