  "scripts": {
    "esl:run": "npx eslint ./ --fix",
    "prebuild": "rimraf dist/",
    "build": "tsc && tsc-alias",
    "dev": "env-cmd -f .env ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules/ --watch docs/ --respawn src/server.ts",
    "test": "env-cmd -f .env jest -i",
    "start": "env-cmd -f .env node dist/server.js",
    "prisma:migration": "env-cmd -f .env npx prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.218.0",
    "@prisma/client": "^4.7.1",
    "aws-sdk": "^2.1263.0",
